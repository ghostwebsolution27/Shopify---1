/* mini page */
.highlight-2 {
  position: relative;
  z-index: 0;
  &:after {
    content: '';
    display: block;
    z-index: 0;
    width: 100%;
    background: #FFC107;
    height: 45px;
    margin-top: -38px;
  }
}

.li_header-mini {
  background: linear-gradient(1250deg, #F2F2F2 67%, #fff 50%) ;
  .text-wrapper-mini {
    text-align: left ;
    h1.header_mini {
      max-width: 300px;
      color: #000;
      font-size: 45px ;
      line-height: 48px ;
    }
  }
}

.ll_protein_title_bar {
  h1 {
    transform: skew(0deg, -8deg);
    -webkit-transform: skew(0deg,-8deg);
    color: white;
    font-family: proxima-nova;
    font-size: 30px;
    text-align: left;
    font-weight: 600;
  }
  &:before {
    background: #171717 ;
    content: '';
    width: 100%;
    min-height: 65px ;
    -webkit-transform: skew(0deg,-8deg);
    transform: skew(0deg,-8deg);
    margin-top: -10px;
    position: absolute;
  }
}  
.ll_mini_product1 {
    background: #FAE184 ;
    padding-top: 126px;
    padding-bottom: 100px;
    &:before {
      background: #FAE184 ;
      content: '';
      width: 100%;
      min-height: 126px ;
      -webkit-transform: skew(0deg, -8deg);
      transform: skew(0deg, -8deg);
      margin-top: -145px;
      position: absolute;
      z-index: 0;
    }
    &:after {
      background: #FAE184 ;
      content: '';
      width: 100%;
      min-height: 126px ;
      -webkit-transform: skew(0deg, -8deg);
      transform: skew(0deg, -8deg);
      margin-top: 25px;
      position: absolute;
      z-index: 0;
    }
    
    & * {
      z-index: 1;
    }
    
    img {
      display: block;
      margin: 0 auto;
    }
    
    .mini_product_title {
      font-family: proxima-nova;
      font-size: 35px;
      line-height: 1.3;
      color: #895918;
      font-weight: 600;
      text-align: center;
      margin-bottom: 25px;
    }
    .mini_product_description {
      font-family: proxima-nova;
