  display: flex;
  -ms-flex-wrap: wrap;
      flex-wrap: wrap;
  -ms-flex: 1 1 100px;
      flex: 1 1 100px;

  @if ($type_body_text_center) {
    -ms-flex-pack: center;
        justify-content: center;
  }
