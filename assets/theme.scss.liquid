// Prices

.product__price {
  color: $colorPrice;
  margin-right: 5px;
}

.product__price--compare {
  padding-right: 5px;
  display: inline-block;
  text-decoration: line-through;
}

.product__price-savings {
  color: $colorTextSavings;
  white-space: nowrap;
}

.product__inventory {
  @include baseSmallFontStack;
  font-style: italic;
  margin: ($gutter / 6) 0;

  @include media-query($medium-up) {
    margin: ($gutter / 4) 0;
  }
