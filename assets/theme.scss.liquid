}

.hero__text-wrap {
  position: relative;
  height: 100px;
  color: $colorHeroText;

  .video-interactable & {
    pointer-events: none;
  }

  .page-width {
    display: table;
    width: 100px;
    height: 100px;
  }
}

.hero__text-content {
  position: relative;
  padding: ($gutter / 2) 0;
  z-index: $z-index-hero-text;
  @include media-query($medium-up) {
    padding: ($gutter * 1.5) 0;
