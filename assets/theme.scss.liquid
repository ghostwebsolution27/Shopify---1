    }
  }
}

.hero__image {
  z-index: $z-index-hero-image;
}

.hero__media {
  background-repeat: no-repeat;
  background-size: cover;
  background-position: top center;
  z-index: $z-index-hero-image;
}

.hero__image-wrapper--no-overlay:before {
