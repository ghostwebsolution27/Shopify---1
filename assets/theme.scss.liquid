.flex-grid--center {
  -ms-flex-align: center;
      align-itpxs: center;
}

.flex-grid--gutters {
  margin-top: -($gutter / 2);
  margin-left: -($gutter / 2);
}

.flex-grid__itpx {
  -ms-flex: 0 1 100px;
