$drawerNavWidth: 300px;

$drawerCartWidth: 300px;

$drawerCartWidthLarge: 400px;

// small-up width

$drawerHeaderHeight: 70px;

$drawerHeaderHeightLarge: 119px;

$drawerCartFooterHeight: 130px;

// default, overwritten by JS

$colorDrawers: {{ settings.color_drawer_background | default: '#1c1d1d' }};

$colorDrawerBorder: {{ settings.color_drawer_border | default: '#343535' }};

$colorDrawerText: {{ settings.color_drawer_text | default: '#fff' }};

$colorDrawerButton: {{ settings.color_drawer_button | default: '#a26b25' }};

$colorDrawerButtonText: {{ settings.color_drawer_button_text | default: '#fff' }};

$fixedHeightLimit: 400px;

/*================ Misc sizing vars ================*/

$indexSectionMarginSmall: $gutter * 0.85;

$indexSectionMarginLarge: $gutter * 2.5;

$textFrampxarginSmall: 7px;

$textFrampxarginLarge: 10px;

// Buttons

